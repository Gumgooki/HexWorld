{
  "name": "hexworld",
  "version": "1.0.0",
  "description": "Instead of making a hex-map from scratch, why not take inspiration from the very planet you live on? HexWorld allows you to transform the breathtaking terrain of Madre Terra into your own hex-maps. No need to stress about if your land formations are realistic, or if there really could be snow that close to the forest; Planet Earth has got your back!",
  "main": "index.js",
  "scripts": {
    "start-dev": "npm run build-watch & npm run start-server",
    "start-server": "nodemon main.js -e html,js,scss --ignore public --ignore client",
    "build-watch": "webpack -w",
    "test": "NODE_ENV='test' mocha  ./tests --recursive --watch --require babel-polyfill --require babel-register",
    "start": "echo 'Running server for production - developers, use npm run start-dev for development' && node main",
    "seed": "node seed.js",
    "build": "webpack"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.2",
    "express": "^4.14.0",
    "p5": "^1.0.0",
    "pg": "^7.4.3",
    "react": "^16.9.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.9.0",
    "react-dropzone": "^11.0.1",
    "react-p5-wrapper": "^2.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.0.0",
    "eslint-plugin-react": "^7.8.2",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "volleyball": "^1.5.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.12"
  }
}
